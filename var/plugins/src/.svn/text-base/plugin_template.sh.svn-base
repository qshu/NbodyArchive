#!/bin/sh

# Template for a script-plugin.
# Thomas Bruesemeister <tbruese@ari.uni-heidelberg.de>

case $1 in

  'pre_execution')
    echo "Sample_plugin: Called pre_execution hook!"
  ;;

  'post_execution')
    echo "Sample_plugin: Called post_execution hook!"
  ;;

  *)
    echo "Sample_plugin: Invalid scriptplugin argument."
  ;;

esac

