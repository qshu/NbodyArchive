          READ(10,*) N
          READ(10,*) DUMDY
          READ(10,*) DUMDY
          PRINT*,' N=',N
          DO 51 I = 1,N
          READ(10,*)BODY(I)
   51     CONTINUE
          PRINT*,' masses read ',BODY(1),BODY(N)
          DO 52 I = 1,N
   52     READ(10,*)(X(K,I),K=1,3)
          DO 53 I = 1,N
   53     READ(10,*)(XDOT(K,I),K=1,3)
